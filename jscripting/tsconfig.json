{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "lib": [
            "es5"
        ],
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../run/ForgeEssentials/JScripting/commands"
    },
    "filesGlob": [
        "mc.d.ts",
        "**.ts"
    ],
    "exclude": [],
    "files": [
        "mc.d.ts",
        "cabin.ts",
        "pumpkinhead.ts",
        "randomtp.ts",
        "sheeprandom.ts",
        "test.ts"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": true
    }
}
